.menu-01{z-index:999}
.menuicon{height: 95rpx; width: 95rpx; line-height: 95rpx; text-align: center; font-style:normal; -webkit-font-smoothing: antialiased;}
.menu-container{display: flex; flex-direction: row; justify-content:space-around;clear:both;}
.menu-item{text-align: center; padding: 25rpx 0;}
.menu-item text{display: block; font-size: 12px; color: #5d5d5d; margin-top: 20rpx}
.menu-item .menuicon{margin: 0 auto}
.menu-01 .fn-item .menuicon{color: #fff; font-size: 50rpx;}
.menu-container01{display: flex; flex-direction: row; justify-content:space-around;position:fixed;bottom: 0;z-index:999;width:100%;background:#fff;border-top:1rpx solid #cfcfcf;height:50px}
.menuicon02{height:65rpx; width:65rpx;  text-align: center; font-style:normal;}
.menu-item02{text-align: center;flex:1;}
.menu-item02 text{display: block; font-size: 12px; color: #5d5d5d;margin-top:-5rpx; line-height: 34rpx;}
.menu-item02 button{position: absolute;width: 100%;height: 100%;left: 0;top: 0;opacity: 0;z-index:9999;}
.menu-item02>view{width:100%;height:100%;display: flex;flex-direction: column;justify-content: space-between;align-items: center;}
.menu-item02>navigator{width:100%;height:100%}