/* 字体 */

@font-face {
  font-family: 'fnicon';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	
}

.fnicon{font-family:"fnicon" !important; height: 95rpx; width: 95rpx; line-height: 95rpx; text-align: center; font-style:normal; -webkit-font-smoothing: antialiased;}
.fn-cart:before{content: "\e600";}
.fn-mark:before{content: "\e601";}
.fn-info:before{content: "\e602";}
.fn-phone:before{content: "\e603";}
.fn-service:before{content: "\e604";}
.fn-file:before{content: "\e605";}
.fn-grid:before{content: "\e606";}
.fn-home:before{content: "\e607";}
.fn-heart:before{content: "\e608";}
.fn-address:before{content: "\e609";}
.fn-search:before{content: "\e60a";}
.fn-balance:before{content: "\e60b";}

.fn-container{display: flex; flex-direction: row; justify-content:space-around;clear:both;}
.fn-item{text-align: center; padding: 25rpx 0;}
.fn-item text{display: block; font-size: 12px; color: #5d5d5d; margin-top: 20rpx}
.fn-item .fnicon{margin: 0 auto}

.fn-01 .fn-item:nth-child(1) .fnicon{background: #4ec0e5}
.fn-01 .fn-item:nth-child(2) .fnicon{background: #ff5367}
.fn-01 .fn-item:nth-child(3) .fnicon{background: #16cb96}
.fn-01 .fn-item:nth-child(4) .fnicon{background: #fe943c}
.fn-01 .fnicon{border-radius: 100px;color: #fff;font-size: 50rpx}

.fn-02 .fn-item .fnicon{  font-size: 60rpx;}
.fn-02 .fn-item:nth-child(1) .fnicon{color: #58b1e9}
.fn-02 .fn-item:nth-child(2) .fnicon{color: #e95d9c}
.fn-02 .fn-item:nth-child(3) .fnicon{color: #efab58}
.fn-02 .fn-item:nth-child(4) .fnicon{color: #45ba61}

.fn-03 .fn-item .fnicon{background: #50c1e9; border-radius:12px;color: #fff; font-size: 50rpx;}


