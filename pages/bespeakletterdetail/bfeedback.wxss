.feedback-box{padding: 30rpx;}
.m-cell {padding:15rpx 5rpx;position:relative;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;}
.m-cells::before{border: none;}
.m-cells::after{border: none;}
.m-cell::before{border: none;}
.btn_box{margin-top: 50rpx;}
.choose{background-color: #fff;}
.choose-grids{ display: flex;flex-wrap: wrap; justify-content: space-around; padding: 50rpx;}
.choose-grids .grid{width: 45%;height: 100rpx;margin-top: 36rpx;border-radius: 6rpx;line-height: 100rpx;text-align: center;border: 2rpx solid #b9dd08;}
.choose-grids .grid:first-child,.choose-grids .grid:nth-of-type(2){margin-top: 0;}
.menuicon{height: 95rpx; width: 95rpx; line-height: 95rpx; text-align: center; font-style:normal; -webkit-font-smoothing: antialiased;}
 .menu-container{display: flex; flex-direction: row; justify-content:space-around;}
.menu-item{text-align: center; padding: 10rpx;float:left;
}
.menu-item text{display: block; font-size: 12px; margin-top: 5rpx}
.menu-item .menuicon{margin: 0 auto}
.menu-01 .fn-item .menuicon{color: #fff; font-size: 50rpx;}
.radio-group{width: 100%;}
.radio-group label.active{color: red; border-bottom: 22px solid red;}
.u-inputs{display: none;}