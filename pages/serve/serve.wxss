page {
	background: #f5f5f5;
}

/*预约列表*/
.list{
  width: 90%;
  height: 220rpx;
  background: #fff;
  margin: auto;
  margin-top: 30rpx;
  border-radius: 10rpx;
  box-shadow: 5px 5px 5px #ececec;
}
.list_s{
  width: 100%;
  height: 116rpx;
  background: #feb9cb;
  display:flex;
  position: relative;

}
.name{
  width: 30%;
  height: 100%;
  margin: auto 0;
  color: #fff;
  line-height: 116rpx;
  font-size: 32rpx;
}
.list_time{
  width:80rpx;
  height:80rpx;
  position:absolute;
  top:14rpx;
  left:30rpx;
  border:4rpx #fafffe solid;
  border-radius: 130rpx;
}

.list_x{
  width: 100%;
  height: 90rpx;
  /* display:flex; */
  margin-top: 30rpx;
}
.list_xl{
  font-size: 30rpx;
  color: #474747;
  height: 50rpx;
  border-radius: 36rpx;
  margin-left: 40rpx;
  float: left;
}
.list_xr{
  font-size: 30rpx;
  color: #f7a9bf;
  height: 50rpx;
  border-radius: 36rpx;
  float: right;
  margin-right: 40rpx;
}
.list_x_t{
  float: left;
  font-size: 30rpx;
  color: #969696;
  margin-left: 40rpx;
  width: 25%;
}