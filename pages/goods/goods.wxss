}
canvas{
 position: fixed;
 z-index: 99;
 top: 0;
 left: 999px;
}

.preview_wrapper{
 z-index: 99;
 position: fixed;
 width: 100%;
 height: 100%;
 display: flex;
 justify-content: center;
 flex-direction: column;
 align-items: center;
 background: rgba(0,0,0,0.4);
}
.btn-group{
 display: flex;justify-content: space-between;
}

.btn-group button{
 flex-grow: 1;
  padding: 0;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 0;
 }
.btn-group button:first-child{
  color: #fff;
  background: #DB384C;
}
.btn-group button:after{
  display: none;
}
.imagesDetail_h image{
  display: block;
  margin: 0 auto;
}