/* pages/reserve/reserve.wxss */
@import "/tpl/template-menu/template-menu.wxss";
.list{
  width: 90%;
  height: 330rpx;
  background: #fff;
  margin: auto;
  margin-top: 50rpx;
  border-radius: 10rpx;
  box-shadow: 5px 5px 5px #ececec;
}
.list_s{
  width: 100%;
  height: 146rpx;
  background: #ffedef;
  display:flex;
  position: relative;

}
.name{
  width: 30%;
  height: 80%;
  margin: auto 0;
}
.list_time{
  width:130rpx;
  height:130rpx;
  position:absolute;
  top:22rpx;
  left:30rpx;
  border:4rpx #fafffe solid;
  border-radius: 130rpx;
}

.list_x{
  width: 100%;
  height: 90rpx;
  /* display:flex; */
  margin-top: 30rpx;
}
.list_xl{
  font-size: 26rpx;
  padding: 0rpx 20rpx;
  background: #ebebeb;
  color: #7b7b7b;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 36rpx;
  margin-left: 30rpx;
  float: left;
}
.list_xr{
  font-size: 34rpx;
  padding: 10rpx 60rpx;
  background: #dd4ecc;
  color: #fff;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 36rpx;
  float: right;
  margin-right: 40rpx;
}
.list_x_t{
  float: left;
  font-size: 30rpx;
  color: #969696;
  margin-left: 20rpx;
  width: 27%;
}
.order-list{position:fixed;bottom:59px;right:10px;border-radius:50%;width:40px;height:40px;z-index:99999;background:#DD4ECC}
.order-listt{position:fixed;bottom:80px;right:10px;border-radius:50%;width:40px;height:40px;z-index:99999;background:#DD4ECC}
