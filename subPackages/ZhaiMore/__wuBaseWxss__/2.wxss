.agencer-box {
    padding: 30rpx 200rpx 30rpx 30rpx;
    background-color: #fff;
    align-items: center;
}

.agencer-avator {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.lineText2 {
    line-height: 40rpx;
    padding-right: 40rpx;
}

.agencer-box .tel {
    padding: 0 40rpx;
    position: absolute;
    right: 0;
}

.icon-dianhua1 {
    font-size: 60rpx;
}

.agencer-box:active {
    background-color: rgba(0,0,0,0.05);
}

.gridYr::before {
    height: 80%;
    top: 10%;
}

.box-content {
    background-color: #fff;
    padding: 0 30rpx;
}

.info-head {
    padding-left: 30rpx;
}

.box-content .flex {
    padding: 30rpx 0;
}

.house-style-input,.placeholder {
    padding-left: 24rpx;
}

.other-desc-textarea {
    width: 100%;
    height: 200rpx;
    padding-top: 24rpx;
}

.other-desc-pic {
    padding-bottom: 40rpx;
    flex-wrap: wrap;
}

.img-zhezhao {
    width: 220rpx;
    height: 180rpx;
    margin: 0 5rpx 20rpx 5rpx;
    position: relative;
    z-index: 10;
}

.img-zhezhao .checked-imgs {
    width: 100%;
    height: 100%;
}

.close-img {
    color: #fff;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    z-index: 100;
    background-color: rgba(0,0,0,0.3);
}

.upload-pic {
    display: inline-block;
    background-color: #e3e3e3;
    height: 180rpx;
    width: 220rpx;
    padding: 40rpx 12rpx 0 0;
}

.upload-pic .icon-camera {
    font-size: 100rpx;
    color: #b2b2b2;
}

.upload-pic:active {
    background-color: rgba(0,0,0,0.19);
}

.increase-pic {
    font-size: 24rpx;
    color: #b2b2b2;
}

.select-list {
    color: #808080;
}

.panel-bg-shade {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    background-color: rgba(0,0,0,0.5);
}

.panel-content {
    width: 96vw;
    position: fixed;
    top: 10vh;
    left: 2vw;
    z-index: 1000;
    border-radius: 1vw;
    background-color: #fff;
}

.panel-content-head {
    padding: 24rpx;
    border-bottom-width: 1rpx;
    border-bottom-style: solid;
}

.common-list {
    max-height: 50vh;
}

.common-item {
    padding: 16rpx 24rpx;
}

.estate-category {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 10;
    height: 8vh;
    background-color: #fff;
}

.estate-category>view {
    padding: 20rpx 0;
}

.estate-category>view:active {
    background: rgba(0,0,0,0.05);
}

.pur-rent-panel-bg-shade {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 8vh;
    left: 0;
    z-index: 100;
    background-color: rgba(0,0,0,0.5);
}

.pur-rent-panel-content {
    width: 100vw;
    max-height: 80vh;
    position: fixed;
    top: 8vh;
    left: 0;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    z-index: 1000;
    background-color: #fff;
}

.position-item {
    padding: 24rpx 30rpx;
}

.orientation-head {
    padding: 0 30rpx;
}

.orientation-list,.area-list,.house-age-list {
    flex-wrap: wrap;
    justify-content: space-between;
}

.orientation-item,.area-item,.house-age-item {
    width: 30%;
    margin-right: 1%;
    padding: 12rpx 20rpx;
}

.submit-btn {
    border: none;
    color: #fff;
}

.display-hide {
    display: none;
}

.display-show {
    display: block;
}

swiper {
    height: 375rpx;
}

.slide-image {
    width: 100%;
    height: 100%;
}

.wx-swiper-dots.wx-swiper-dots-horizontal {
    position: absolute;
    bottom: -10rpx;
}

.nodata {
    color: #ccc;
    flex-flow: column wrap;
    justify-content: center;
    align-items: center;
    padding: 200rpx 0;
}

.nodata .icon-kongbaiyemian {
    font-size: 200rpx;
}

.vrBtn {
    width: 500rpx;
    margin: 20 auto 5rpx;
    color: #fff;
    background-color: #f66;
    border-radius: 100rpx;
    font-size: 32rpx;
}

.vrBtn::after {
    border: none;
}

.vrBtn:active {
    opacity: 0.85;
}

.isTop {
    position: absolute;
    top: 30rpx;
    left: 30rpx;
    font-size: 12px;
    line-height: 20px;
    padding: 0 14rpx;
    color: #fff;
    background-color: rgba(0,0,0,0.6);
}