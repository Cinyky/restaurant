}

button[type="warn"][plain] {
    border: none;
    font-size: 12px;
    border-radius: 6rpx;
}

button[type="warn"][plain]::before {
}

button[type="warn"][plain]:active {
    color: #fff;
}

.estate-box {
    background-color: #fff;
}

.do-type-box {
    padding: 50rpx 0;
}

.do-type {
    width: 70%;
    height: 80rpx;
    margin: 0 auto;
}

.do-type>text {
    height: 100%;
    line-height: 80rpx;
    text-align: center;
    display: inline-block;
}

.actice-style {
    color: #fff;
    border-radius: 50rpx;
}

.estate-item {
    padding: 30rpx;
    background-color: #fff;
}

.estate-item:active {
    background-color: rgba(0,0,0,0.05);
}

.estate-pic {
    width: 220rpx;
    height: 220rpx;
}

.item-content {
    position: relative;
    justify-content: space-between;
}

.estate-feature {
    color: #444;
    font-weight: bold;
}

.estate-name {
    color: #636363;
}

.estate-address,.estate-contain,.estate-area {
    color: #777;
}

.estate-advantage>text {
    font-size: 12px;
    padding: 0 20rpx;
    line-height: 22px;
    float: left;
    transform-origin: left center;
    transform: scale(0.8);
    border-radius: 6rpx;
}

.estate-advantage>text:nth-child(n+2) {
}

.price-cou-unt {
    float: right;
}

.price-count {
    font-size: 42rpx;
}

.price-contain,.price-area,.price-unit,.estate-na-add {
    font-size: 24rpx;
}

.shop-item {
    padding: 30rpx 30rpx 24rpx 30rpx;
    background-color: #fff;
}

.shop-pic {
    width: 180rpx;
    height: 140rpx;
}

.shop-address {
    line-height: 40rpx;
}

