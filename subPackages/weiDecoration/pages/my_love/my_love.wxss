@import "/tpl/template-menu/template-menu.wxss";
@import "../../assets/common.wxss";

page, view, button, text, form, input {
  margin: 0;
  padding: 0;
  /*box-sizing: border-box;*/
}

page, .page {
  background: #f5f5f5;
  font-size: 30rpx;
}

.page{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.nav{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.nav>view{
  flex-grow: 1;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}

